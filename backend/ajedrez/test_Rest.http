################################################################
# REGISTRO DE USUARIO
################################################################
POST http://localhost:8090/api/usuarios/register
Content-Type: application/json

{
  "nickname": "pedro",
  "email": "pedro@example.com",
  "password": "Password1!"
}

################################################################
# LOGIN DE USUARIO (BASIC AUTH)
# La cabecera Authorization se genera en Base64: nickname:password
################################################################
POST http://localhost:8090/api/usuarios/login
Content-Type: application/json
Authorization: Basic amuanOlBhc3N3b3JkQDEyMw==

{
  "nickname": "pedro",
  "password": "Password1!"
}

################################################################
# OBTENER INFO DE USUARIO POR NICKNAME
################################################################
GET http://localhost:8090/api/usuarios/pedro
Content-Type: application/json
Authorization: Basic amuanOlBhc3N3b3JkQDEyMw==

################################################################
# COMPROBAR SI EXISTE NICKNAME
################################################################
GET http://localhost:8090/api/usuarios/exists/nickname/juan
Content-Type: application/json

################################################################
# COMPROBAR SI EXISTE EMAIL
################################################################
GET http://localhost:8090/api/usuarios/exists/email/juan@example.com
Content-Type: application/json

